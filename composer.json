{
    "name": "completesolar/api-clients",
    "type": "project",
    "description": "Logic to give access to your app using API keys.",
    "license": "MIT",
    "require": {
        "php": "8.1.*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "7.*",
        "illuminate/contracts": "^10.0",
        "illuminate/database": "^10.0",
        "illuminate/routing": "^10.0",
        "illuminate/support": "^10.0",
        "illuminate/view": "^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "9.*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "CompleteSolar\\ApiClients\\ApiClientServiceProvider"
            ]
        }
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-4": {
            "CompleteSolar\\ApiClients\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CompleteSolar\\ApiClientsTests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
